source_set("regex") {
  deps = [ "//zircon/third_party/ulib/musl:musl_internal" ]
  sources = [
    "fnmatch.c",
    "glob.c",
  ]

  configs -= [ "//build/config:symbol_visibility_hidden" ]

  # TODO(https://fxbug.dev/42136089): delete the below and fix compiler warnings
  configs += [ "//build/config:Wno-conversion" ]
}
