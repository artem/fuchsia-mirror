{
    expose: [
        {
            // Exposes zoneinfo files.
            // Users should use this as path `/config/tzdata/zoneinfo`.
            directory: "pkg",
            from: "framework",
            as: "tzdata-zoneinfo",
            subdir: "data/tzdata/zoneinfo",
        },
    ],
}
