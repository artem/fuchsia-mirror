import("//build/zircon/zx_library.gni")

group("tests") {
  testonly = true
  deps = [ "test:fake-object-test" ]
}

zx_library("fake-object") {
  sdk = "static"
  sdk_headers = [ "lib/fake-object/object.h" ]
  sdk_publishable = "internal"

  sources = [
    "fake-handle.cc",
    "fake-object.cc",
  ]
  public_deps = [
    "//sdk/lib/fdio",
    "//zircon/system/ulib/zircon-internal",
    "//zircon/system/ulib/zx",
  ]
}
