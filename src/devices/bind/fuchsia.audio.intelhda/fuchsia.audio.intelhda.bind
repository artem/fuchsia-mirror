library fuchsia.audio.intelhda;

extend uint fuchsia.BIND_PROTOCOL {
  IHDA_DEVICE = 76,
  IHDA_CODEC = 77,
  IHDA_DSP = 78,
};

// Codec Vendor IDs.
extend uint fuchsia.BIND_IHDA_CODEC_VID {
  INTEL = 0x8086,    // Intel
  REALTEK = 0x10ec,  // Realtek
  RED_HAT = 0x1af4,  // Red Hat, Inc.
};
