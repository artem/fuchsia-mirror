library fuchsia.runtime.microbenchmarks;

using zx;

/// A fake FIDL device server for testing microbenchmarks against.
@transport("Driver")
closed protocol Device {
    /// Replies immediately with an empty response.
    strict Handshake() -> () error zx.Status;
};
