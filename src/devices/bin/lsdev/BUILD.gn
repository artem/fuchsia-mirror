import("//build/assembly/bootfs_files_for_assembly.gni")

executable("lsdev") {
  visibility = [ ":*" ]
  sources = [ "lsdev.cc" ]
  deps = [
    "//sdk/fidl/fuchsia.device:fuchsia.device_cpp_wire",
    "//sdk/lib/component/incoming/cpp",
    "//sdk/lib/fdio",
  ]
}

bootfs_files_for_assembly("bootfs") {
  deps = [ ":lsdev" ]
}
