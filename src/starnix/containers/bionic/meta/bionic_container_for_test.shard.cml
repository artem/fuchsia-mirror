        external_container: "true",
    },
    children: [
        {
            name: "container",
            url: "bionic#meta/bionic_container.cm",
            environment: "#starnix_runner_env",
        },
    ],
    use: [
        {
            protocol: "fuchsia.component.runner.ComponentRunner",
            from: "#container",
        },
    ],
}
