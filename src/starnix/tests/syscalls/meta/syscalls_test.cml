{
    include: [ "//src/starnix/tests/starnix_test_with_expectations.shard.cml" ],
    program: {
        binary: "data/tests/syscalls_test_bin",
        external_container: "true",
        uid: "0",
        test_type: "gtest",
    },
    children: [
        {
            name: "test_container",
            url: "#meta/debian_container.cm",
            environment: "#starnix_runner_env",
        },
    ],
    use: [
        {
            protocol: "fuchsia.component.runner.ComponentRunner",
            from: "#test_container",
        },
        {
            storage: "custom_artifacts",
            rights: [ "rw*" ],
            path: "/custom_artifacts",
        },
    ],
}
