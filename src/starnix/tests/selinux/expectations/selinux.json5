{
    actions: [
        {
            type: "expect_pass",
            matchers: [
                "*",
            ],
        },
        {
            type: "expect_failure",
            matchers: [
                "*selinux_entrypoint*",
                "*selinux_execshare*",
                "*selinux_exectrace*",

                // TODO(http://b/330904217): update `execute_no_trans` expectation to pass once filesystems are labeled correctly.
                "*selinux_execute_no_trans*",
                "*selinux_fdreceive*",
                "*selinux_inherit*",
                "*selinux_link*",
                "*selinux_mkdir*",
                "*selinux_msg*",
                "*selinux_open*",
                "*selinux_readlink*",
                "*selinux_relabel*",
                "*selinux_rename*",
                "*selinux_rxdir*",
                "*selinux_sem*",
                "*selinux_setattr*",
                "*selinux_setnice*",
                "*selinux_shm*",
                "*selinux_stat*",
                "*selinux_sysctl*",
                "*selinux_task_setnice*",
                "*selinux_task_getsid*",
                "*selinux_file*",
                "*selinux_ioctl*",
                "*selinux_capable_file*",
                "*selinux_capable_net*",
                "*selinux_capable_sys*",
                "*selinux_dyntrans*",
                "*selinux_dyntrace*",
                "*selinux_bounds*",
                "*selinux_nnp_nosuid*",
                "*selinux_mmap*",
                "*selinux_inet_socket*",
                "*selinux_checkreqprot*",
                "*selinux_mac_admin*",
                "*selinux_atsecure*",
                "*selinux_netlink_socket*",
                "*selinux_prlimit*",
                "*selinux_bpf*",
                "*selinux_keys*",
                "*selinux_key_socket*",
                "*selinux_cgroupfs_label*",
                "*selinux_notify*",
                "*selinux_module_load*",
                "*selinux_tun_tap*",
                "*selinux_perf_event*",
            ],
        },
        {
            type: "skip",
            matchers: [
                // Test sometimes hangs.
                "*selinux_unix_socket*",
            ],
        },
    ],
}
