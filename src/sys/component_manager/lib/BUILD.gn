group("non_hermetic_tests") {
  testonly = true
  deps = [
    "builtins:tests",
    "logger:tests",
  ]
}

group("hermetic_tests") {
  testonly = true
  deps = [
    "hooks:tests",
    "processargs:tests",
    "sandbox:tests",
    "serve_processargs:tests",
    "util:tests",
  ]
}
