{
    include: [ "syslog/client.shard.cml" ],
    program: {
        binary: "bin/simple_elf_program",
    },
    children: [
        {
            name: "elf_runner",
            url: "fuchsia-builtin://#elf_runner.cm",
        },
    ],
    use: [
        {
            runner: "elf",
            from: "#elf_runner",
        },
    ],
    offer: [
        {
            protocol: "fuchsia.process.Launcher",
            from: "parent",
            to: "#elf_runner",
        },
    ],
}
