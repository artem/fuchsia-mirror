{
    include: [
        "//src/sys/test_runners/rust/default.shard.cml",
        "inspect/client.shard.cml",
        "syslog/client.shard.cml",
    ],
    program: {
        binary: "bin/debug_dash_launcher_test",
    },
    children: [
        {
            name: "debug-dash-launcher",
            url: "#meta/debug_dash_launcher.cm",
        },
        {
            name: "ptysvc",
            url: "#meta/ptysvc.cm",
        },
        {
            name: "mock-resolver",
            url: "#meta/mock_resolver.cm",
        },
    ],
    use: [
        {
            protocol: "fuchsia.dash.Launcher",
            from: "#debug-dash-launcher",
        },
    ],
    offer: [
        {
            protocol: "fuchsia.pkg.PackageResolver",
            from: "#mock-resolver",
            as: "fuchsia.pkg.PackageResolver-full",
            to: "#debug-dash-launcher",
        },
        {
            protocol: "fuchsia.logger.LogSink",
            from: "parent",
            to: "#mock-resolver",
        },
        {
            storage: "tmp",
            from: "parent",
            to: "#mock-resolver",
        },
        {
            protocol: [
                "fuchsia.logger.LogSink",
                "fuchsia.process.Launcher",
            ],
            from: "parent",
            to: "#debug-dash-launcher",
        },
        {
            protocol: "fuchsia.boot.WriteOnlyLog",
            from: "parent",
            to: "#ptysvc",
        },
        {
            protocol: "fuchsia.hardware.pty.Device",
            from: "#ptysvc",
            to: "#debug-dash-launcher",
        },
        {
            protocol: "fuchsia.sys2.RealmQuery",
            from: "framework",
            to: "#debug-dash-launcher",
        },
    ],
}
