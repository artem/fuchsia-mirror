{
    include: [
        "//src/sys/base-resolver/tests/as-a-component-resolver/meta/root-base.cml",
        "inspect/offer.shard.cml",
    ],
    children: [
        {
            name: "fake_dependencies",
            url: "#meta/fake-dependencies-fxblob.cm",
        },
    ],
    offer: [
        {
            protocol: "fuchsia.fxfs.BlobCreator",
            from: "#fake_dependencies",
            to: "#base_resolver",
        },
        {
            protocol: "fuchsia.fxfs.BlobReader",
            from: "#fake_dependencies",
            to: "#base_resolver",
        },
        {
            protocol: "fuchsia.kernel.VmexResource",
            from: "parent",
            to: "#base_resolver",
        },
    ],
}
