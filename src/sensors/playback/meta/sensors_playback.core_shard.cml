{
    children: [
        {
            name: "sensors_playback",
            url: "fuchsia-pkg://fuchsia.com/sensors/playback#meta/playback.cm",
            startup: "eager",
            environment: "#core-env",
        },
    ],
    use: [
        {
            protocol: [ "fuchsia.hardware.sensors.Driver" ],
            from: "#sensors_playback",
        },
        {
            protocol: [ "fuchsia.hardware.sensors.Playback" ],
            from: "#sensors_playback",
        },
    ],
    offer: [
        {
            protocol: [ "fuchsia.logger.LogSink" ],
            from: "parent",
            to: [ "#sensors_playback" ],
        },
    ],
}
