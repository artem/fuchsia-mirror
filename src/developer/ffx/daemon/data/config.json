{
    "daemon": {
        "autostart": true
    },
    "discovery": {
        "zedboot": {
            "enabled": false
        },
        "mdns": {
            "enabled": true
        }
    },
    "fastboot": {
        "flash": {
            "min_timeout_secs": 60,
            "timeout_rate": 5
        }
    },
    "ssh": {
        "auth-sock": [
            "$SSH_AUTH_SOCK"
        ],
        "priv": [
            "$FUCHSIA_SSH_KEY",
            "$HOME/.ssh/fuchsia_ed25519"
        ],
        "pub": [
            "$FUCHSIA_AUTHORIZED_KEYS",
            "$HOME/.ssh/fuchsia_authorized_keys"
        ]
    }
}
