{
  "name": "fs-tree",
  "description": "Run a power-failure test on a mutable fs (e.g.  Fxfs or minfs). Load pattern: generate, move, and delete random directory trees.",
  "examples": [],
  "flags": [
    {
      "kind": "Switch",
      "optionality": "optional",
      "long": "--help",
      "short": null,
      "description": "display usage information",
      "hidden": false
    },
    {
      "kind": {
        "Option": {
          "arg_name": "device-path"
        }
      },
      "optionality": "optional",
      "long": "--device-path",
      "short": "p",
      "description": "the path of the block device on the target device to use for testing. If one isn't provided the test will pick an appropriate one. WARNING: the test can (and likely will!) format this device. Don't use a main system partition!",
      "hidden": false
    },
    {
      "kind": {
        "Option": {
          "arg_name": "format"
        }
      },
      "optionality": "required",
      "long": "--format",
      "short": null,
      "description": "which filesystem format to target.",
      "hidden": false
    },
    {
      "kind": {
        "Option": {
          "arg_name": "seed"
        }
      },
      "optionality": "optional",
      "long": "--seed",
      "short": "s",
      "description": "a seed to use for all random operations. Tests are NOT deterministic relative to the provided seed. The operations will be identical, but because of the non-deterministic timing-dependent nature of the tests, the exact time the reboot is triggered in relation to the operations is not guaranteed. One will be randomly generated if not provided. When performing the same test multiple times in one run, a new seed will be generated for each run if one was not provided.",
      "hidden": false
    },
    {
      "kind": {
        "Option": {
          "arg_name": "relay"
        }
      },
      "optionality": "optional",
      "long": "--relay",
      "short": "r",
      "description": "path to a power relay for cutting the power to a device. Probably the highest-numbered /dev/ttyUSB[N]. If in doubt, try removing it and seeing what disappears from /dev. When a relay is provided, the harness automatically switches to use hardware reboots.",
      "hidden": false
    },
    {
      "kind": "Switch",
      "optionality": "optional",
      "long": "--dmc-reboot",
      "short": null,
      "description": "use dmc to trigger reboots. Only works in infra.",
      "hidden": false
    },
    {
      "kind": {
        "Option": {
          "arg_name": "iterations"
        }
      },
      "optionality": "optional",
      "long": "--iterations",
      "short": "i",
      "description": "run the test N number of times, collecting statistics on the number of failures.",
      "hidden": false
    },
    {
      "kind": "Switch",
      "optionality": "optional",
      "long": "--run-until-failure",
      "short": "f",
      "description": "run the test until a verification failure is detected, then exit.",
      "hidden": false
    },
    {
      "kind": "Switch",
      "optionality": "optional",
      "long": "--bootserver",
      "short": null,
      "description": "run a bootserver to serve netboot images to a device. This will only work in infra!",
      "hidden": false
    }
