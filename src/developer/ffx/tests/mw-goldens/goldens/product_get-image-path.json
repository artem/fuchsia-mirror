{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CommandStatus",
  "description": "CommandStatus is returned to indicate exit status of a command. The Ok variant returns the list of artifacts.",
  "oneOf": [
    {
      "description": "Successful execution with an optional informational string.",
      "type": "object",
      "required": [
        "ok"
      ],
      "properties": {
        "ok": {
          "type": "object",
          "required": [
            "path"
          ],
          "properties": {
            "path": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Unexpected error with string.",
      "type": "object",
      "required": [
        "unexpected_error"
      ],
      "properties": {
        "unexpected_error": {
          "type": "object",
          "required": [
            "message"
          ],
          "properties": {
            "message": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "description": "A known kind of error that can be reported usefully to the user",
      "type": "object",
      "required": [
        "user_error"
      ],
      "properties": {
        "user_error": {
          "type": "object",
          "required": [
            "message"
          ],
          "properties": {
            "message": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    }
  ]
}