{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ShowCmdInstance",
  "type": "object",
  "required": [
    "moniker",
    "url"
  ],
  "properties": {
    "moniker": {
      "$ref": "#/definitions/Moniker"
    },
    "url": {
      "type": "string"
    },
    "environment": {
      "type": [
        "string",
        "null"
      ]
    },
    "instance_id": {
      "type": [
        "string",
        "null"
      ]
    },
    "resolved": {
      "anyOf": [
        {
          "$ref": "#/definitions/ShowCmdResolvedInfo"
        },
        {
          "type": "null"
        }
      ]
    }
  },
  "definitions": {
    "Moniker": {
      "title": "String",
      "type": "string"
    },
    "ShowCmdResolvedInfo": {
      "type": "object",
      "required": [
        "collections",
        "exposed_capabilities",
        "incoming_capabilities",
        "resolved_url"
      ],
      "properties": {
        "resolved_url": {
          "type": "string"
        },
        "merkle_root": {
          "type": [
            "string",
            "null"
          ]
        },
        "incoming_capabilities": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "exposed_capabilities": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "config": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "$ref": "#/definitions/ConfigField"
          }
        },
        "started": {
          "anyOf": [
            {
              "$ref": "#/definitions/ShowCmdExecutionInfo"
            },
            {
              "type": "null"
            }
          ]
        },
        "collections": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "ConfigField": {
      "description": "A single structured configuration key-value pair.",
      "type": "object",
      "required": [
        "key",
        "value"
      ],
      "properties": {
        "key": {
          "type": "string"
        },
        "value": {
          "type": "string"
        }
      }
    },
    "ShowCmdExecutionInfo": {
      "type": "object",
      "required": [
        "outgoing_capabilities",
        "runtime",
        "start_reason"
      ],
      "properties": {
        "runtime": {
          "$ref": "#/definitions/Runtime"
        },
        "outgoing_capabilities": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "start_reason": {
          "type": "string"
        }
      }
    },
    "Runtime": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "Unknown"
          ]
        },
        {
          "type": "object",
          "required": [
            "Elf"
          ],
          "properties": {
            "Elf": {
              "type": "object",
              "required": [
                "job_id"
              ],
              "properties": {
                "job_id": {
                  "type": "integer",
                  "format": "uint64",
                  "minimum": 0.0
                },
                "process_id": {
                  "type": [
                    "integer",
                    "null"
                  ],
                  "format": "uint64",
                  "minimum": 0.0
                },
                "process_start_time": {
                  "type": [
                    "integer",
                    "null"
                  ],
                  "format": "int64"
                },
                "process_start_time_utc_estimate": {
                  "type": [
                    "string",
                    "null"
                  ]
                }
              }
            }
          },
          "additionalProperties": false
        }
      ]
    }
  }
}