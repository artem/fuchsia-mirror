{
    include: [ "syslog/client.shard.cml" ],
    program: {
        runner: "elf",
        binary: "bin/system_activity_governor",
    },
    capabilities: [
        {
            protocol: [ "fuchsia.power.suspend.Stats" ],
        },
    ],
    expose: [
        {
            protocol: [ "fuchsia.power.suspend.Stats" ],
            from: "self",
        },
    ],
}
