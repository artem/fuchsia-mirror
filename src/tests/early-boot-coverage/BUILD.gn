group("tests") {
  testonly = true

  # End to End tests that do not contribute to coverage.
  # Disabled in ASAN builds because the unstripped artifacts are dependant on the variant path.
  if (!is_coverage && !is_asan && host_os == "linux") {
    deps = [ "standalone-test" ]
  }
}
