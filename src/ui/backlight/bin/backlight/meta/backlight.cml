    include: [
        "syslog/client.shard.cml",
        "trace/use.shard.cml",
    ],
    program: {
        runner: "elf",
        binary: "bin/backlight",
    },
    use: [
        {
            directory: "dev-backlight",
            rights: [ "r*" ],
            path: "/dev/class/backlight",
        },
    ],
}
