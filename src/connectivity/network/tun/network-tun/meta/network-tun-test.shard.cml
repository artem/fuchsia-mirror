{
    children: [
        {
            name: "network-tun",
            url: "#meta/network-tun.cm",
        },
    ],
    use: [
        {
            protocol: [ "fuchsia.net.tun.Control" ],
            from: "#network-tun",
        },
    ],
    offer: [
        {
            protocol: [
                "fuchsia.inspect.InspectSink",
                "fuchsia.logger.LogSink",
            ],
            from: "parent",
            to: "#network-tun",
        },
    ],
}
