{
    include: [
        "//src/connectivity/network/testing/netemul/runner/default.shard.cml",
        "inspect/offer.shard.cml",
    ],
    program: {
        networks: [],
        netstacks: [],
    },
    children: [
        {
            name: "test-driver",
            url: "#meta/rust-test-driver.cm",
        },
    ],
    use: [
        {
            protocol: "fuchsia.test.Suite",
            from: "#test-driver",
        },
    ],
    offer: [
        {
            protocol: "fuchsia.logger.LogSink",
            from: "parent",
            to: "#test-driver",
        },
    ],
}
