{
    actions: [
        {
            type: "expect_pass",
            matchers: [
                "*",
            ],
        },
        {
            type: "expect_failure",
            matchers: [
                // TODO(https://fxbug.dev/124960): Support fuchsia.net.neighbor in NS3.
                "cant_hang_twice_ns3",
                "channel_is_closed_if_not_polled_ns3",
                "neigh_add_remove_entry_ns3",
                "neigh_clear_entries_ns3",
                "neigh_list_entries_ns3",
                "neigh_unreachability_config_errors_ns3",
                "neigh_unreachability_config_ns3",
                "neigh_unreachable_entries_ns3",
                "neighbor_with_many_addresses_disconnects_ns3",
                "remove_device_clears_neighbors_ns3",

                // TODO(https://fxbug.dev/135498): Validate addresses in NS2.
                "neigh_add_remove_entry_invalid_addr_ns2*",
                "neigh_add_entry_invalid_mac_ns2*",
            ],
        },
    ],
}
