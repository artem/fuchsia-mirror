{
    actions: [
        {
            type: "expect_pass",
            matchers: [
                "*",
            ],
        },
        {
            type: "expect_failure",
            matchers: [
                // TODO(https://fxbug.dev/42084185): Remove once interface
                // authorization is implemented in NS3.
                "interface_authorization_fails_with_invalid_token_*_ns3*",
                "authorizing_for_one_interface_out_of_two_*_ns3*",
                "unauthenticated_connections_cannot_remove_routes_*_ns3*",
            ],
        },
    ],
}
