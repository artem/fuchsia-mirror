{
    actions: [
        {
            type: "expect_pass",
            matchers: [
                "*",
            ],
        },
        {
            type: "expect_failure",
            matchers: [
                // TODO(https://fxbug.dev/42056881, https://fxbug.dev/42058290): Should pass
                // against Netstack3 once adding deprecated addresses and updating
                // address lifetimes are supported.
                "address_deprecation_ns3",

                // TODO(https://fxbug.dev/42156951): Should pass against Netstack3 once
                // multicast forwarding is supported.
                "get_set_forwarding_ns3*multicast*",
                "get_set_forwarding_loopback_ns3::set_ip_false_and_multicast_ip_true",
                "get_set_forwarding_loopback_ns3::set_multicast*",
            ],
        },
    ],
}
