{
    actions: [
        {
            type: "expect_pass",
            matchers: [
                "*",
            ],
        },
        {
            type: "expect_failure",
            matchers: [
                // TODO(https://fxbug.dev/42057933): Implement filtering in NS3.
                "*ns3*",
            ],
        },
        {
            type: "skip",
            matchers: [
                // TODO(https://fxbug.dev/42057933): Implement filtering in NS3.
                "drop_udp*_ns3",
            ],
        },
    ],
}
