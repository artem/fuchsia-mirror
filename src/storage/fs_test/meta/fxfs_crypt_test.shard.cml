{
    children: [
        {
            name: "fxfs-crypt",
            url: "#meta/fxfs-crypt.cm",
        },
    ],
    use: [
        {
            protocol: "fuchsia.fxfs.Crypt",
            from: "#fxfs-crypt",
        },
        {
            protocol: "fuchsia.fxfs.CryptManagement",
            from: "#fxfs-crypt",
        },
    ],
    offer: [
        {
            protocol: [ "fuchsia.logger.LogSink" ],
            from: "parent",
            to: "#fxfs-crypt",
        },
        {
            protocol: "fuchsia.inspect.InspectSink",
            from: "parent",
            to: [ "#fxfs-crypt" ],
        },
    ],
}
