group("tests") {
  testonly = true
  deps = [
    "fxt:tests",
    "perf_publish:tests",
    "perfmon:tests",
    "trace_processing:tests",
  ]
  if (current_cpu == "arm64" || current_cpu == "x64") {
    deps += [ "gperftools:tests" ]
  }
}
