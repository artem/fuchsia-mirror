{
    include: [
        "//src/sys/test_runners/gtest/default.shard.cml",
        "syslog/client.shard.cml",
    ],
    program: {
        binary: "bin/cpuperf_integration_tests",
    },
    use: [
        {
            protocol: [ "fuchsia.process.Launcher" ],
        },
        {
            directory: "sys",
            rights: [ "r*" ],
            path: "/dev/sys",
        },
        {
            storage: "data",
            path: "/data",
        },
        {
            storage: "tmp",
            path: "/tmp",
        },
    ],
}
