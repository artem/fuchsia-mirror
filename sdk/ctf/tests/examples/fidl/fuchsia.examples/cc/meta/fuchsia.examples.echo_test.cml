{
    include: [
        "//sdk/ctf/meta/test.shard.cml",
        "inspect/offer.shard.cml",
        "sys/testing/elf_test_runner.shard.cml",
        "syslog/client.shard.cml",
    ],
    program: {
        binary: "bin/fuchsia.examples.echo_test_main",
    },
    children: [
        {
            name: "test_realm",
