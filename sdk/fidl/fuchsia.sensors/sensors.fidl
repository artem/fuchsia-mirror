library fuchsia.sensors;

type SensorId = struct {
    id int32;
};

type Sensor = table {
    1: id SensorId;
    2: name string:MAX;
};

@discoverable
open protocol Manager {
    flexible GetSensorsInfo() -> (struct {
        sensors vector<Sensor>:MAX;
    });
};
