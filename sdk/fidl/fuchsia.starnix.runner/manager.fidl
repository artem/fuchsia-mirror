@available(added=HEAD)
library fuchsia.starnix.runner;

@discoverable
open protocol Manager {
    /// Suspends all the running Starnix kernels.
    flexible Suspend(table {});

    /// Resumes all suspended Starnix kernels.
    flexible Resume(table {});
};
