{
    actions: [
        {
            type: "expect_pass",
            matchers: [
                "*",
            ],
        },
        {
            include: "../loopback_isolated_tcp_fin_wait_filter.json5",
        },
        {
            type: "skip",
            matchers: [
                // NS3 does not support dual-stack TCP sockets yet, and these
                // tests are known to occasionally timeout rather than fail
                // (e.g. https://fxbug.dev/130377).
                "All/SocketInetLoopbackIsolatedTest.TCPActiveCloseTimeWaitTest/ListenV6Any_ConnectV4Any",
            ],
        },
        {
            type: "expect_failure",
            matchers: [
                // Netstack3 does not support dual-stack TCP sockets yet.
                "All/SocketInetLoopbackIsolatedTest.*/ListenV4Any_ConnectV4MappedAny",
                "All/SocketInetLoopbackIsolatedTest.*/ListenV4Any_ConnectV4MappedLoopback",
                "All/SocketInetLoopbackIsolatedTest.*/ListenV4Loopback_ConnectV4MappedLoopback",
                "All/SocketInetLoopbackIsolatedTest.*/ListenV4MappedAny_ConnectV4Any",
                "All/SocketInetLoopbackIsolatedTest.*/ListenV4MappedAny_ConnectV4Loopback",
                "All/SocketInetLoopbackIsolatedTest.*/ListenV4MappedAny_ConnectV4MappedAny",
                "All/SocketInetLoopbackIsolatedTest.*/ListenV4MappedAny_ConnectV4MappedLoopback",
                "All/SocketInetLoopbackIsolatedTest.*/ListenV4MappedLoopback_ConnectV4Any",
                "All/SocketInetLoopbackIsolatedTest.*/ListenV4MappedLoopback_ConnectV4Loopback",
                "All/SocketInetLoopbackIsolatedTest.*/ListenV4MappedLoopback_ConnectV4MappedLoopback",
                "All/SocketInetLoopbackIsolatedTest.*/ListenV6Any_ConnectV4Any",
                "All/SocketInetLoopbackIsolatedTest.*/ListenV6Any_ConnectV4Loopback",
                "All/SocketInetLoopbackIsolatedTest.*/ListenV6Any_ConnectV4MappedAny",
                "All/SocketInetLoopbackIsolatedTest.*/ListenV6Any_ConnectV4MappedLoopback",
            ],
        },
    ],
}
