{
    actions: [
        {
            type: "expect_pass",
            matchers: [
                "*",
            ],
        },
        {
            include: "../loopback_tcp_backlog_filter.json5",
        },
        {
            type: "expect_failure",
            matchers: [
                // TODO(https://issues.fuchsia.dev/319117141): Support listening
                // in both stacks.
                "All/SocketInetLoopbackTest.TCPBacklogAcceptAll/ListenV6Any_ConnectV4*",
                "All/SocketInetLoopbackTest.TCPBacklog/ListenV6Any_ConnectV4*",
            ],
        },
    ],
}
